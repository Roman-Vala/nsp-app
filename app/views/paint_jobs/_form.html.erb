<%= form_for(paint_job) do |f| %>

  <div class="form-group">
    <%= f.label :customer %>
    <%= f.text_field :customer, class: "form-control" %>
  </div>

  <div class="form-group">
    <%= f.label :description %>
    <%= f.text_area :description, class: "form-control" %>
  </div>

  <div class="">
    <%= f.submit "submit", class: "btn btn-primary" %>
  </div>
<% end %>
